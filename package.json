{
  "name": "vocational-directory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "deploy": "git add . && git commit -m 'Update: Deploy latest changes' && git push origin main",
    "dev:fresh": "pkill -f 'next dev' || true && npm run dev",
    "test:local": "curl -s http://localhost:3000/api/students | jq . || echo 'Server not running or no data'"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
